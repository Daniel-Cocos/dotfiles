{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "transient_prompt": {
    "template": " ❯ {{ .AbsolutePWD }} : ",
    "foreground": "#f8f8f2",
    "background": "transparent",
    "newline": true
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\n\u256d\u2500",
          "foreground": "#f8f8f2",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\udb85\udc0b",
          "foreground": "#ff5555",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ .Icon }}  ",
          "foreground": "#f8f8f2",
          "type": "os",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "full"
          },
          "template": " \udb80\ude56 {{ path .Path .Location }}",
          "foreground": "#f8f8f2",
          "type": "path",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "branch_icon": "",
            "cache_duration": "none",
            "display_changing_color": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "full_branch_path": true
          },
          "template": "\ue725 ({{ url .UpstreamIcon .UpstreamURL }} {{ url .HEAD .UpstreamURL }}){{ if gt .Ahead 0 }}<#50fa7b> +{{ .Ahead }}</>{{ end }}{{ if gt .Behind 0 }}<#ff5555> -{{ .Behind }}</>{{ end }}{{ if .Working.Changed }}<#f8f8f2> \uf044 {{ .Working.String }}</>{{ end }}{{ if .Staging.Changed }}<#f8f8f2> \uf046 {{ .Staging.String }}</>{{ end }}",
          "foreground": "#f8f8f2",
          "type": "git",
          "style": "plain"
        },
        {
          "template": "\ue718 {{ .Full }}",
          "foreground": "#f8f8f2",
          "type": "node",
          "style": "plain"
        },
        {
          "properties": {
            "display_mode": "files",
            "extensions": [
              "package-lock.json"
            ]
          },
          "template": "\ue71e {{ .Full }}",
          "foreground": "#f8f8f2",
          "type": "npm",
          "style": "plain"
        },
        {
          "properties": {
            "display_mode": "files",
            "extensions": [
              "pnpm-lock.yaml"
            ]
          },
          "template": "\udb80\udec1 {{ .Full }}",
          "foreground": "#f8f8f2",
          "type": "pnpm",
          "style": "plain"
        },
        {
          "properties": {
            "display_mode": "files"
          },
          "template": "\ue73c {{ .Full }}",
          "foreground": "#f8f8f2",
          "type": "python",
          "style": "plain"
        },
        {
          "template": "\ue738 {{ .Full }}",
          "foreground": "#f8f8f2",
          "type": "java",
          "style": "plain"
        },
        {
          "template": "\ue73d {{ .Full }}",
          "foreground": "#f8f8f2",
          "type": "php",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "\u2570\u2500 ❯",
          "foreground": "#f8f8f2",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "patch_pwsh_bleed": true,
  "final_space": true
}

